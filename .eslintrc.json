{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "indent": [
            "tab"
        ],
        "linebreak-style": [
            "windows"
        ],
        "quotes": [
            "single"
        ],
        "semi": [
            "always"
        ]
    }
}
